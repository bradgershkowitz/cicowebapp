{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules" : {
        "no-console": "off",
         "quotes": [
            "error",
            "double"
          ],
          "max-len": "off",
          "no-underscore-dangle": "off"
      },
      "extends": [
        "airbnb-base",
        "prettier"
      ]
}